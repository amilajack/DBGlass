    "draft-js": "^0.9.1",
    "draft-js-export-markdown": "^0.2.2",
    "draft-js-import-markdown": "^0.2.1",
    "electron-debug": "^1.1.0",
    "electron-json-storage": "^3.0.0",
    "fixed-data-table": "^0.6.3",
    "font-awesome": "^4.7.0",
    "jwt-simple": "^0.5.1",
    "ladda": "^1.0.0",
    "open-ssh-tunnel": "^0.3.1",
    "pg": "^6.1.2",
    "pg-native": "^1.10.0",
    "react": "^15.4.1",
    "react-addons-pure-render-mixin": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-date-picker": "^5.3.28",
    "react-dom": "^15.4.1",
    "react-modal-dialog": "^4.0.4",
    "react-redux": "^5.0.1",
    "react-select": "^1.0.0-rc.2",
    "react-syntax-highlighter": "^4.0.1",
    "recompose": "^0.21.2",
    "redux": "^3.6.0",
    "redux-form": "^6.4.3",
    "redux-thunk": "^2.1.0",
    "request": "^2.79.0",
    "revalidate": "git://github.com/web-pal/revalidate.git#forTemUsage",
    "semver": "^5.3.0",
    "source-map-support": "^0.4.8"
  },
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=3.x"
  }
}
